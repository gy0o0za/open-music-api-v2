# Server
HOST=localhost
PORT=5000

# JWT
ACCESS_TOKEN_KEY=your_access_token_secret_here
REFRESH_TOKEN_KEY=your_refresh_token_secret_here
# in seconds
ACCESS_TOKEN_AGE=3600

# PostgreSQL
PGHOST=127.0.0.1
PGPORT=5432
PGUSER=postgres
PGPASSWORD=postgres
PGDATABASE=openmusic
# Or use a single URL instead of the above PG* vars
# DATABASE_URL=postgres://postgres:postgres@127.0.0.1:5432/openmusic

# Optional
# NODE_ENV=development
